{
  "Guacd": {
    "Hostname": "127.0.0.1",
    "Port": 4822
  },
  "Socket": {
    "Port": 8080,
    "MaxInactivityAllowedInMin": 10
  },
  "Client": {
    "DefaultArguments": {
      "rdp": {
        "args": "connect",
        "port": 3389,
        "width": 1024,
        "height": 768,
        "dpi": 96,
        "security": "any",
        "ignore-cert": "true"
      },
      "vnc": {
        "args": "connect",
        "port": 5900,
        "width": 1024,
        "height": 768,
        "dpi": 96
      },
      "ssh": {
        "args": "connect",
        "port": 22,
        "width": 1024,
        "height": 768,
        "dpi": 96
      },
      "telnet": {
        "args": "connect",
        "port": 23,
        "width": 1024,
        "height": 768,
        "dpi": 96
      }
    },
    "UnencryptedArguments": {
      "rdp": [ "width", "height", "dpi" ],
      "vnc": [ "width", "height", "dpi" ],
      "ssh": [ "color-scheme", "font-name", "font-size", "width", "height", "dpi" ],
      "telnet": [ "color-scheme", "font-name", "font-size", "width", "height", "dpi" ]
    }
  }
}